{
  "name": "paskuale75/yii2-notifications",
  "description": "PHP 8.3+ compatible fork of webzop/yii2-notifications",
  "type": "yii2-extension",
  "keywords": ["yii2", "notifications"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Paskuale75",
      "email": "pellicanipasquale@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "yiisoft/yii2": "~2.0.13",
    "minishlink/web-push": "^6.0"
  },
  "repositories": [
    {
        "type": "composer",
        "url": "https://asset-packagist.org"
    }
    ],
  "autoload": {
    "psr-4": {
      "webzop\\notifications\\": ""
    }
  },
  "extra": {
    "yii\\composer\\Plugin": {
      "bootstrap": "webzop\\notifications\\Bootstrap"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
